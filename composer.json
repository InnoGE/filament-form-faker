{
    "name": "innoge/filament-form-faker",
    "description": "A package to generate fake input data for Filament forms.",
    "keywords": [
        "InnoGE",
        "laravel",
        "filament-form-faker"
    ],
    "homepage": "https://github.com/innoge/filament-form-faker",
    "license": "MIT",
    "authors": [
        {
            "name": "Tim Geisendoerfer",
            "email": "geisi@users.noreply.github.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.1",
        "spatie/laravel-package-tools": "^1.14.0",
        "illuminate/contracts": "^10.0|^11.0",
        "filament/forms": "^3.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.8|^8.0",
        "nunomaduro/larastan": "^2.0.1",
        "orchestra/testbench": "^8.17|^9.0",
        "pestphp/pest": "^2.28",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan-deprecation-rules": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "InnoGE\\FilamentFormFaker\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "InnoGE\\FilamentFormFaker\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "clear": "@php vendor/bin/testbench package:purge-filament-form-faker --ansi",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "build": [
            "@composer run prepare",
            "@php vendor/bin/testbench workbench:build --ansi"
        ],
        "start": [
            "Composer\\Config::disableProcessTimeout",
            "@composer run build",
            "@php vendor/bin/testbench serve"
        ],
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "InnoGE\\FilamentFormFaker\\FilamentFormFakerServiceProvider"
            ],
            "aliases": {
                "FilamentFormFaker": "InnoGE\\FilamentFormFaker\\Facades\\FilamentFormFaker"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
